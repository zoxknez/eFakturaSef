{
  "version": 2,
  "installCommand": "npm install",
  "buildCommand": "cd /vercel/path0 && npm run build --workspace=shared && npm run build --workspace=frontend",
  "outputDirectory": "frontend/dist",
  "framework": null,
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
