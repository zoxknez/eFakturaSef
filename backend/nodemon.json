{
  "watch": ["src"],
  "ext": "ts,json",
  "ignore": ["src/**/*.test.ts", "logs/**/*", "node_modules/**/*", "dist/**/*"],
  "delay": 2000,
  "env": {
    "NODE_ENV": "development",
    "PRISMA_QUERY_LOG": "false",
    "DEBUG": "false"
  },
  "exec": "ts-node --transpile-only src/index.ts",
  "legacyWatch": false,
  "verbose": false,
  "restartable": "rs",
  "events": {
    "restart": "echo 'ðŸ”„ Backend restartovan...'"
  }
}
